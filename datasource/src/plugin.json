{
  "type": "datasource",
  "name": "Signal K",
  "id": "signalk-datasource",
  "metrics": false,
  "streaming": true,
  "routes": [
    {
      "path": "historyapi",
      "url": "http://{{ .JsonData.hostname }}/",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.token }}"
        }
      ]
    },
    {
      "path": "shistoryapi",
      "url": "https://{{ .JsonData.hostname }}/",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.token }}"
        }
      ]
    },
    {
      "path": "noauthhistoryapi",
      "url": "http://{{ .JsonData.hostname }}/"
    },
    {
      "path": "snoauthhistoryapi",
      "url": "https://{{ .JsonData.hostname }}/"
    }
  ],
  "info": {
    "description": "Signal K datasource (streaming & historical data)",
    "author": {
      "name": "teppo.kurki@Ä±ki.fi",
      "url": "http://signalk.org/"
    },
    "keywords": ["datasource"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/tkurki/signalk-grafana"
      }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
  }
}
